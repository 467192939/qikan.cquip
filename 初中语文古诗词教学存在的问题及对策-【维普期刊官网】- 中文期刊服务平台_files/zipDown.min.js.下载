function isDown(n,t,i,r){var u={};u.year=n;u.num=t;u.gch=i;u.articleId=r;u.t=(new Date).getTime();$.ajax({type:"post",url:"/Qikan/Article/ShowDwon",dataType:"json",data:u,success:function(n){var t=n.RetValue.limit;t&&$("#rightimgdownzip").css("display","inline-block")}})}function showZipDown(n){var t=[n];window.checklogin(showZipDownAfterLogin,t)}function showZipDownAfterLogin(n){$.post("/Qikan/Article/GetArticleRight",{articleId:n,kind:6},function(t){var r=t.RetValue,i;r?getZipDownInfo(n):t.Tag==0?(i="/Qikan/UserPay/CheckZipOrder?id="+n+"&kind=6",window.open(i,"_blank")):t.Tag==1&&layer.alert("包库权限不足")})}function getZipDownInfo(n){$("#searcher").hasClass("fixed-top")&&$("#searcher").removeClass("fixed-top");var t=document.getElementById("body").offsetWidth,i={y:0,scrollY:0,x:0,scrollX:0,width:t};html2canvas(document.querySelector("#pagescreenshot"),i).then(function(t){var u=t.toDataURL(),r=u.split(";"),e=r[0].split(":")[1],f=r[1].split(",")[1],i=$("<form><\/form>").attr("action","/Qikan/Article/ZipDown").attr("method","post");i.append($("<input><\/input>").attr("type","hidden").attr("name","id").attr("value",n));i.append($("<input><\/input>").attr("type","hidden").attr("name","imagebase64").attr("value",f));i.appendTo("body").submit().remove();layer.msg("下载中",{icon:16,shade:.01,time:1e4})})}